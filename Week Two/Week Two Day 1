# 2: Matrix Dot Product

# Import numpy
import numpy as np
# Define the three arrays
array1 = np.array([[6.-9,1],[4,24,8]])
array2 = np.array([[1,0],[0,1]])
array3 = np.array([[6,-9,1],[4,24,8]])
array4 = np.array([[4,3],[3,2]])
array5 = np.array([[-2,3],[3,-4]])

solution1 = 2 * array1
solution2 = np.dot(array2,array3)
solution3 = np.dot(array4,array5)


print(solution1,solution2,solution3)
# Compute dot products via the .dot function

